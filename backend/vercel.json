

{
    "version": 2,
    "name": "Full Stack",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  